{
	"name": "HermitCore Playground",
	"dockerFile": "Dockerfile",
	"extensions": [
		"rust-lang.rust",
		"bungcip.better-toml",
		"vadimcn.vscode-lldb"
	],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": {
		"lldb.executable": "/usr/bin/lldb-3.9",
		"terminal.integrated.shell.linux": "/bin/bash"
	}
}
